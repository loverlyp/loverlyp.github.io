    <id>https://krt.moe/posts/%E5%89%8D%E7%AB%AF/%E5%B7%A5%E5%85%B7%E4%BD%BF%E7%94%A8/git%E5%91%BD%E4%BB%A4/</id>
    <published>2025-07-08T07:17:00.000Z</published>
